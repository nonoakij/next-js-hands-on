{
  "name": "@next-js-hands-on/docs",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "astro": "astro",
    "build": "astro check && astro build",
    "check": "biome check -apply ./*",
    "dev": "astro dev --port $npm_package_config_port",
    "format": "biome format --write ./*",
    "lint": "biome lint ./*",
    "preview": "astro preview",
    "start": "astro dev"
  },
  "config": {
    "port": 5050
  },
  "dependencies": {
    "@astrojs/check": "0.9.3",
    "@astrojs/starlight": "0.27.1",
    "astro": "4.15.6",
    "sharp": "0.33.5",
    "typescript": "5.6.2"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.1"
  }
}
