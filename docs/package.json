{
  "name": "@next-js-hands-on/docs",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "astro": "astro",
    "build": "astro check && astro build",
    "check": "biome check -apply ./*",
    "dev": "astro dev --port $npm_package_config_port",
    "format": "biome format --write ./*",
    "lint": "biome lint ./*",
    "preview": "astro preview",
    "start": "astro dev"
  },
  "config": {
    "port": 5050
  },
  "dependencies": {
    "@astrojs/check": "0.7.0",
    "@astrojs/starlight": "0.23.1",
    "astro": "4.9.1",
    "sharp": "0.32.6",
    "typescript": "5.4.5"
  },
  "devDependencies": {
    "@biomejs/biome": "1.7.3"
  }
}
